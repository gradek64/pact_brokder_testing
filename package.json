{
  "name": "jest",
  "version": "1.0.0",
  "description": "Jest Pact example",
  "main": "index.js",
  "scripts": {
    "warn": "echo '!!...turn off your wifi for this test...!!'",
    "test:consumer": "jest __tests__/ --runInBand && npm run warn",
    "test:provider": "jest provider/provider.spec.js",
    "test": "TEST_INDEX=0 jest __tests__/ --runInBand --setupFiles ./pactSetup.js --setupTestFrameworkScriptFile ./pactTestWrapper.js --config ./ENV_CONFIG.js",
    "publishManual:pact": "curl -XPUT -H \"Content-Type: application/json\" -d@pacts/myconsumer-cards-myprovider.json http://127.0.0.1:80/pacts/provider/MyProvider/consumer/MyConsumer-Cards/version/1.0.0",
    "publish:pact": "node ./publishScript.js",
    "deleteBunch:pact": "node ./deletePactsScript.js",
    "delete:pact": "curl -X DELETE http://127.0.0.1:80/pacticipants/MyConsumer-Login"
  },
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "axios": "0.14.0",
    "dotenv": "6.1.0",
    "jest-cli": "21.0.0",
    "request": "2.88.0"
  },
  "dependencies": {
    "@pact-foundation/pact-node": "6.16.1",
    "@types/bunyan": "1.8.3",
    "@types/cli-color": "0.3.29",
    "@types/lodash": "4.14.73",
    "bunyan": "1.8.12",
    "bunyan-prettystream": "0.1.3",
    "cli-color": "^1.1.0",
    "cors": "2.8.1",
    "es6-object-assign": "1.1.0",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.1",
    "lodash": "4.17.4",
    "lodash.isfunction": "3.0.8",
    "lodash.isnil": "4.0.0",
    "lodash.isundefined": "3.0.1",
    "lodash.omit": "4.5.0",
    "lodash.omitby": "4.6.0",
    "pkginfo": "0.4.1",
    "popsicle": "9.2.0"
  }
}
