{
  "consumer": {
    "name": "MyConsumer-Login"
  },
  "provider": {
    "name": "MyProvider"
  },
  "interactions": [
    {
      "description": "perform login",
      "providerState": "login consumer works",
      "request": {
        "method": "GET",
        "path": "/login",
        "query": "login=Greg",
        "headers": {
          "Accept": "application/json; charset=utf-8"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "login": "Greg"
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    }
  }
}